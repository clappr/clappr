# editorconfig.org

# top-most EditorConfig file
root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true

indent_style = space
indent_size = 2

trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = true

[{package.json}]
indent_style = space
indent_size = 2
insert_final_newline = true
